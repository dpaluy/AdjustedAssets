- title "Setting: #{@setting.name}" 

%p
  %b Multiplier:
  = @setting.multiplier
%p
  %b Stock fee:
  = @setting.stock_fee
%p
  %b Option fee:
  = @setting.option_fee
%p
  %b Supplement stock:
  = @setting.supplement_stock
%p
  %b Asset adjustment:
  = @setting.asset_adjustment
%p
  %b Points to rehedge:
  = @setting.points_to_rehedge

- if user_signed_in?
  = link_to 'Edit', edit_setting_path(@setting)
  \|
= link_to 'Back', settings_path
